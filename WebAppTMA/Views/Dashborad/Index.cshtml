            TMAdb db = new TMAdb();
            var usr = db.users.Where(x => x.Email == a).FirstOrDefault();
            




        }
        <h1 style="margin-left:250px; text-align:center">Hi @usr.Name</h1>
      
        <h3 id="clock" style="margin-left:250px;"></h3>
            @*<div ></div>*@

            <script>
                var c = setInterval(time, 1000)
                function time() {
                    var d = new Date();
                    var t = d.toLocaleTimeString();
                    document.getElementById('clock').innerHTML = t;
                }


            </script>
        
        <p style="margin-left:290px; text-align:center">Wellcom to the Dashboard . Here you can change the funtionality of the site<br /> and you can add and remove the uses and role</p>
