@{
    ViewBag.Title = "About";
}
<head>
    <style>
        /*--about--*/
        /*.about, .services, .gallery, .events, .team, .newsletter, .contact {
            padding: 5em 0;
        }*/

        .about-top-grid, .abt-btm-grid {
            padding: 0;
        }

        .about-top-text {
            padding: 1em;
            text-align: center;
        }

            .about-top-text h3 {
                font-size: 3em;
                color: #016773;
                text-transform: capitalize;
                letter-spacing: 2px;
            }

            .about-top-text p {
                font-size: 0.9em;
                color: #212121;
                line-height: 2.2em;
                /*margin: 1em 0 0 0;*/
                letter-spacing: 2px;
                word-spacing: 2px;
            }

        .w3ls-ma {
            position: relative;
            margin: -4px;
        }

        .w3ls-ma1 {
            position: relative;
            margin: -4px;
        }

        .layer {
            position: absolute;
            bottom: 100%;
            left: 0;
            right: 0;
            background-color: rgba(3, 51, 107, 0.4);
            overflow: hidden;
            width: 100%;
            height: 0;
            transition: .5s ease;
            -webkit-transition: 0.5s all;
            -o-transition: 0.5s all;
            -moz-transition: 0.5s all;
        }

        .w3ls-ma:hover .layer {
            bottom: 0;
            height: 100%;
        }

        /*--//about--*/
    </style>
</head>
<section class="about" id="about">
    <div class="container">
        <div class="w3ls-about-grids">
            <div class="col-md-4 col-sm-6 about-top-grid w3ls-ma">
                <img src="~/images/pictma4.jpg" class="img-responsive" alt="imges" />
                <div class="layer">
                </div>
            </div>

            <div class="col-md-4 col-sm-6 about-top-text">
                <h3 style="margin-top:-8px;">about us</h3>
                @*<h5 style="margin-top:-9px;">Wellcom to our organization</h5>*@
                <p style="margin-top:-16px;">
                    Monitoring and supervising the performances of government offices and reporting to the district government
                    Spatial planning
                    Executing and managing development plans
                    Controlling land development
                    Enforcing laws, rules, and bye-laws
                    Maintaining databases and information systems
                    Collecting taxes, fines, and penalties

                </p>
                <div class="layer">
                </div>
            </div>
            <div class="col-md-4 col-sm-12 about-top-grid w3ls-ma">
                <img src="~/images/pictma1.jpg" class="img-responsive" alt="imges" />
                <div class="layer">
                </div>
            </div>

            @*<div class="clearfix"></div>
                @*<div @*class="col-md-5 col-sm-5 abt-btm-grid w3ls-ma1">
                    <img src="~/images/pictma5 (1).jpg"  class="img-responsive" alt="images"/>
                    <div class="layer">
                    </div>*@




        </div>
        <div @*class="col-md-7 col-sm-7 abt-btm-grid w3ls-ma1*@ class="col-md-12 abt-btm-grid w3ls-ma">
            <img src="~/images/pictma5 (2).jpg" class="img-responsive" alt="images" style="width:1200px; margin-top:-5px;" />
            <div class="layer">

            </div>
        </div>
        <div class="clearfix"></div>

    </div>


</section>


